#compdef lid-metrics
typeset -A opt_args
local context state line ret curcontext="$curcontext"
_arguments -S "--fabric[Comma-separated Fabric names]:fabric:_fabrics" "--fabrics[Comma-separated Fabric names]:fabrics:_fabrics" "-f[Comma-separated Fabric names]:f:_fabrics" "--fabric-group[The comma-separated FabricGroup names]:fabric-group:" "-fg[The comma-separated FabricGroup names]:fg:" "--days[Number of days to look at {default: 7 days}]:days:" "-d[Number of days to look at {default: 7 days}]:d:" "--start[Start date to consider {default: N days from --end}]:start:" "--end[End date to consider {default: today}]:end:" "--partition[How to partition requests]:partition:" "--series[What to use for series]:series:" "-s[What to use for series]:s:" "--columns[Which columns to show]:columns:" "-c[Which columns to show]:c:" "--note[Annotate each series with values from a partitioner]:note:" "--hour[Convenience shortcut for --series hour]:hour:" "--product[Comma-separated list of products to report on]:product:_product" "-p[Comma-separated list of products to report on]:p:_product" "*--exclude[Allows to exclude specified action, origin, product_tag or user]:exclude:" "--html[Show colors in HTML (this is only useful for updating wiki page doc examples...)]:html:" "--csv[Format result as csv]:csv:" "--log[Log file destination]:log:" "--debug[Enable debug logging]:debug:" ": : _message 'deployables = [DEPLOYABLES]...'"
compdef _lid-metrics lid-metrics
