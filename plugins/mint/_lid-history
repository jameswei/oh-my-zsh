#compdef lid-history
typeset -A opt_args
local context state line ret curcontext="$curcontext"
_arguments -S "--fabric[Comma-separated Fabric names]:fabric:_fabrics" "--fabrics[Comma-separated Fabric names]:fabrics:_fabrics" "-f[Comma-separated Fabric names]:f:_fabrics" "--fabric-group[The comma-separated FabricGroup names]:fabric-group:" "-fg[The comma-separated FabricGroup names]:fg:" "--verbose[Print a multi-line representation of each request.]:verbose:" "--limit[The number of requests to show (per fabric)]:limit:" "-l[The number of requests to show (per fabric)]:l:" "--sort[Request attribute to sort by]:sort:" "-s[Request attribute to sort by]:s:" "--actions[Comma-separated actions to show ("only show these actions")]:actions:" "-a[Comma-separated actions to show ("only show these actions")]:a:" "--canary[Show only canary actions.]:canary:" "-c[Show only canary actions.]:c:" "--exclude-actions[Comma-separated actions to ignore ("show all actions except these")]:exclude-actions:" "-x[Comma-separated actions to ignore ("show all actions except these")]:x:" "--partial[Partial match on request name]:partial:" ": : _message 'deployables = [DEPLOYABLES]...'"
compdef _lid-history lid-history
